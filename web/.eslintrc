{
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "parser": "@babel/eslint-parser",
  "plugins": ["import", "unused-imports"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "requireConfigFile": false
  },
  "rules": {
    "no-console": "warn",
    "no-eval": "error",
    "no-undef": "off",
    "no-unused-vars": "warn",
    "import/first": "error"
  }
}